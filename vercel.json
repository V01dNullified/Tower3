{
  "version": 2,
  "buildCommand": "node vercel-build.js",
  "installCommand": "npm install",
  "outputDirectory": "dist/public",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}